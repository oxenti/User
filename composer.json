{
    "name": "oxenti/user",
    "description": "User plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0",
        "admad/cakephp-jwt-auth": "1.0.x-dev",
        "happyr/linkedin-api-client": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "User\\": "./plugins/User/src"
        }
    },
    "extra": {
        "installer-paths": {
            "./plugins/User": ["User"],
        }
    },
    "autoload-dev": {
        "psr-4": {
            "User\\Test\\": "./plugins/User/tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
